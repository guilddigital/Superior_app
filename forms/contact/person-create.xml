      <bind nodeset="/data/person/sex" type="string" calculate="../c_sex"/>
      <bind nodeset="/data/person/today_d" type="string" calculate="format-date-time(today(),&quot;%d&quot;)"/>
      <bind nodeset="/data/person/c_name" type="string" required="true()"/>
      <bind nodeset="/data/person/c_sex" type="select1" required="true()"/>
      <bind nodeset="/data/person/dob_method" type="select1" required="true()"/>
      <bind nodeset="/data/person/dob_calendar" type="date" required="true()" jr:constraintMsg="Invalid Date" constraint=".&lt;today() and floor(difference-in-months(., today() ) div 12 ) &lt;= 120" relevant="selected(../dob_method,'calendar')"/>
      <bind nodeset="/data/person/age_years" type="int" required="true()" jr:constraintMsg="Invalid Age Value" constraint=". &gt;= 0 and .&lt;=120" relevant="selected(../dob_method,'approx')"/>
      <bind nodeset="/data/person/ephemeral_years" type="string" calculate="format-date-time(today(),&quot;%Y&quot;) -../age_years"/>
      <bind nodeset="/data/person/dob_approx" type="string" calculate="concat(string(../ephemeral_years),'-',string(format-date-time(today(), &quot;%m&quot;)),'-',string(format-date-time(today(), &quot;%d&quot;)))"/>
      <bind nodeset="/data/person/dob_raw" type="string" calculate="if(not(selected( ../dob_method,'approx')), 
../dob_calendar,
../dob_approx)"/>
      <bind nodeset="/data/person/c_dob_iso" type="string" calculate="format-date-time(../dob_raw,&quot;%Y-%m-%d&quot;)"/>
      <bind nodeset="/data/person/c_dob_debug" readonly="true()" type="string" relevant="0"/>
      <bind nodeset="/data/person/current_age" type="string" calculate="floor(difference-in-months(date( /data/person/c_dob_iso ),today()) div 12)"/>
      <bind nodeset="/data/person/date_vht_visit" type="string" calculate=" /data/person/p_date_vht_visit "/>
      <bind nodeset="/data/person/has_disability" type="string" calculate=" /data/person/p_has_disability "/>
      <bind nodeset="/data/person/test_for_hiv_last3months" type="string" calculate=" /data/person/p_test_for_hiv_last3months "/>
      <bind nodeset="/data/person/hiv_test_result" type="string" calculate=" /data/person/p_hiv_test_result "/>
      <bind nodeset="/data/person/on_art_treatment" type="string" calculate=" /data/person/p_on_art_treatment "/>
      <bind nodeset="/data/person/has_tb" type="string" calculate=" /data/person/p_has_tb "/>
      <bind nodeset="/data/person/on_tb_treatment" type="string" calculate=" /data/person/p_on_tb_treatment "/>
      <bind nodeset="/data/person/phone1" type="string" required="false()" jr:constraintMsg="Mobile number should be 10 digits and should start with 07 or 039" constraint="regex(.,'^(07)\d{8}$|^(039)\d{7}$')" relevant=" /data/person/current_age &gt;18"/>
      <bind nodeset="/data/person/phone2" type="string" required="false()" jr:constraintMsg="Mobile number should be 10 digits and should start with 07 or 039" constraint="regex(.,'^(07)\d{8}$|^(039)\d{7}$')" relevant=" /data/person/current_age &gt;18 and  /data/person/phone1  != ''"/>
      <bind nodeset="/data/person/p_date_vht_visit" type="date" required="true()" jr:constraintMsg="VHT Visit Date can't be more than 7 days ago. Also can't be a future date" constraint=". &lt;= today() and (floor(decimal-date-time(.)) &gt;= (floor(decimal-date-time(today()) - 7)))"/>
      <bind nodeset="/data/person/relationship_with_hh" type="select1" required="true()" relevant=" /data/person/current_age !='' and  /data/person/c_sex !=''"/>
      <bind nodeset="/data/person/other_relationship" type="string" required="true()" relevant=" /data/person/relationship_with_hh ='other'"/>
      <bind nodeset="/data/person/client_category" type="select1" required="true()"/>
      <bind nodeset="/data/person/nationality" type="select1" required="true()" relevant="not(selected(../client_category, 'national'))"/>
      <bind nodeset="/data/person/other_nationality" type="string" required="true()" relevant="selected(../nationality, 'other')"/>
      <bind nodeset="/data/person/registered" type="select1" relevant="not(selected(../client_category, 'national'))"/>
      <bind nodeset="/data/person/individual_number" type="string" relevant="selected(../registered, 'yes') and not(selected(../client_category, 'national'))"/>
      <bind nodeset="/data/person/group_number" type="string" relevant="not(selected(../client_category, 'national'))"/>
      <bind nodeset="/data/person/nin" type="string" relevant="selected(../client_category, 'national')"/>
      <bind nodeset="/data/person/p_has_disability" type="select1" required="true()"/>
      <bind nodeset="/data/person/disability" type="select" required="true()" relevant=" /data/person/p_has_disability  = 'yes'"/>
      <bind nodeset="/data/person/child_in_school" type="select1" required="true()" relevant=" /data/person/relationship_with_hh !='spouse' and  /data/person/current_age &gt;=3 and  /data/person/current_age &lt;=50"/>
      <bind nodeset="/data/person/p_test_for_hiv_last3months" type="select1" required="true()"/>
      <bind nodeset="/data/person/hiv_test_result_label" type="string" calculate="if( /data/person/p_test_for_hiv_last3months ='yes', 
&quot;What was the result of the HIV test?&quot;,
&quot;What is the client's HIV status?&quot;)"/>
      <bind nodeset="/data/person/p_hiv_test_result" type="select1" required="true()" relevant=" /data/person/p_test_for_hiv_last3months !=''"/>
      <bind nodeset="/data/person/p_on_art_treatment" type="select1" required="true()" relevant=" /data/person/p_hiv_test_result ='positive'"/>
      <bind nodeset="/data/person/note_attend_art_clinic" readonly="true()" type="string" relevant=" /data/person/p_on_art_treatment  = 'no'"/>
      <bind nodeset="/data/person/taking_medication" type="select1" required="true()" relevant=" /data/person/p_on_art_treatment  = 'yes'"/>
      <bind nodeset="/data/person/note_explain_importance_taking_med" readonly="true()" type="string" required="false()" relevant="../taking_medication = 'no'"/>
      <bind nodeset="/data/person/note_encourage_to_continue_taking_med" readonly="true()" type="string" relevant="../taking_medication = 'yes'"/>
      <bind nodeset="/data/person/note_encourage_client" readonly="true()" type="string" relevant=" /data/person/p_hiv_test_result ='negative'"/>
      <bind nodeset="/data/person/note_advise_client_check_status" readonly="true()" type="string" relevant=" /data/person/p_hiv_test_result ='unknown' and  /data/person/p_test_for_hiv_last3months ='yes'"/>
      <bind nodeset="/data/person/note_advise_client_check_status2" readonly="true()" type="string" relevant=" /data/person/p_hiv_test_result ='unknown' and  /data/person/p_test_for_hiv_last3months ='no'"/>
      <bind nodeset="/data/person/p_has_tb" type="select1" required="true()"/>
      <bind nodeset="/data/person/has_cough" type="select1" required="true()" relevant=" /data/person/p_has_tb  = 'no'"/>
      <bind nodeset="/data/person/p_on_tb_treatment" type="select1" required="true()" relevant=" /data/person/p_has_tb  = 'yes'"/>
      <bind nodeset="/data/person/note_tb_referral" readonly="true()" type="string" relevant=" /data/person/p_has_tb  = 'yes' and  /data/person/p_on_tb_treatment  = 'no'"/>
      <bind nodeset="/data/person/received_hpv" type="select1" required="true()" relevant=" /data/person/c_sex  = 'female' and  /data/person/current_age  &gt;= 10 and  /data/person/current_age  &lt;= 24"/>
      <bind nodeset="/data/person/received_tt_vaccine" type="select1" required="true()" relevant=" /data/person/c_sex  = 'female' and  /data/person/current_age  &gt;= 10 and  /data/person/current_age  &lt;= 24"/>
      <bind nodeset="/data/person/has_hypertension" type="select1" required="true()" relevant=" /data/person/current_age  &gt; 5"/>
      <bind nodeset="/data/person/has_sickle_cell" type="select1" required="true()"/>
      <bind nodeset="/data/person/has_diabetes" type="select1" required="true()"/>
      <bind nodeset="/data/person/uses_tobacco" type="select1" required="true()" relevant=" /data/person/current_age &gt;=12"/>
      <bind nodeset="/data/person/sleep_under_llin" type="select1" required="true()"/>
      <bind nodeset="/data/person/why_not_using_llin" type="select1" required="true()" relevant="selected(../sleep_under_llin, 'no')"/>
      <bind nodeset="/data/person/why_not_using_llin_other" type="string" required="true()" relevant="selected(../why_not_using_llin, 'other')"/>
      <bind nodeset="/data/person/note_vht_assist_how_to_get_llin" readonly="true()" type="string" relevant="selected(../why_not_using_llin, 'has_no_llin') or selected(../why_not_using_llin, 'not_enough_llins')"/>
      <bind nodeset="/data/person/note_vht_demonstrate_on_llin_use" readonly="true()" type="string" relevant="selected(../why_not_using_llin, 'does_not_know_how_to_use')"/>
      <bind nodeset="/data/person/using_fp_method" type="select1" required="true()" relevant=" /data/person/current_age &gt;=15 and  /data/person/current_age &lt;=50"/>
      <bind nodeset="/data/person/note_fp_registration" readonly="true()" type="string" relevant=" /data/person/using_fp_method  = 'yes'"/>
      <bind nodeset="/data/person/menstrual_material" type="select1" required="true()" relevant=" /data/person/c_sex  = 'female' and  /data/person/current_age  &gt;= 12 and  /data/person/current_age  &lt;= 20"/>
      <bind nodeset="/data/person/menstrual_material_label" type="string" calculate="concat(if(selected( /data/person/menstrual_material , 'disposable_pads'), 'packets of ', 'pieces of '),jr:choice-name( /data/person/menstrual_material ,' /data/person/menstrual_material '))"/>
      <bind nodeset="/data/person/mm_quantity" type="int" required="true()" constraint=". &gt; 0" relevant="../menstrual_material != ''"/>
      <bind nodeset="/data/person/have_pads" type="select1" relevant=" /data/person/c_sex  = 'female' and  /data/person/current_age  &gt;= 12 and  /data/person/current_age  &lt;= 20"/>
      <bind nodeset="/data/person/num_pads" type="int" constraint=". &gt; 0" relevant="../have_pads = 'yes'"/>
      <bind nodeset="/data/person/reusable_pads" type="select1" relevant=" /data/person/c_sex  = 'female' and  /data/person/current_age  &gt;= 12 and  /data/person/current_age  &lt;= 20"/>
      <bind nodeset="/data/person/num_reusable_pads" type="int" constraint=". &gt; 0" relevant="../reusable_pads = 'yes'"/>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/data/inputs">
      <label></label>
      <group ref="/data/inputs/user">
        <label></label>
        <input ref="/data/inputs/user/facility_id">
          <label>Facility ID for the parent user</label>
        </input>
        <input ref="/data/inputs/user/contact_id">
          <label>Contact ID for the parent user</label>
        </input>
        <input ref="/data/inputs/user/language">
          <label>Language</label>
        </input>
      </group>
    </group>
    <group ref="/data/person">
      <input appearance="hidden" ref="/data/person/type">
        <label>Person Type</label>
        <hint>mandatory</hint>
      </input>
      <input appearance="hidden" ref="/data/person/parent">
        <label>Parent</label>
        <hint>mandatory</hint>
      </input>
      <input ref="/data/person/c_name">
        <label>Names</label>
        <hint>Please include both names</hint>
      </input>
      <select1 appearance="horizontal" ref="/data/person/c_sex">
        <label>Sex</label>
        <item>
          <label>Female</label>
          <value>female</value>
        </item>
        <item>
          <label>Male</label>
          <value>male</value>
        </item>
      </select1>
      <select1 appearance="horizontal" ref="/data/person/dob_method">
        <label>Please choose method to select date of birth</label>
        <item>
          <label>Date (preferred)</label>
          <value>calendar</value>
        </item>
        <item>
          <label>Current age</label>
          <value>approx</value>
        </item>
      </select1>
      <input ref="/data/person/dob_calendar">
        <label>Date of Birth</label>
        <hint>Date must be either today or before today</hint>
      </input>
      <input ref="/data/person/age_years">
        <label>Age (in years)</label>
        <hint>For under 5's use date of birth option</hint>
      </input>
      <input ref="/data/person/c_dob_debug">
        <label>Year: ../ephemeral_years
DOB Approx: ../dob_approx
DOB Calendar: ../dob_calendar
DOB ISO: ../c_dob_iso</label>
      </input>
      <input appearance="number" ref="/data/person/phone1">
        <label>Phone number</label>
        <hint>Mobile number should take this format 07XXXXXXXX or 039XXXXXXX</hint>
      </input>
      <input appearance="number" ref="/data/person/phone2">
        <label>Alternative phone number</label>
        <hint>Mobile number should take this format 07XXXXXXXX or 039XXXXXXX</hint>
      </input>
      <input ref="/data/person/p_date_vht_visit">
        <label>Date of VHT Visit</label>
      </input>
      <select1 appearance="horizontal" ref="/data/person/relationship_with_hh">
        <label>What is the Relationship with Household Head?</label>
        <itemset nodeset="instance('select_relationship')/root/item[((prefecture!=3 and  /data/person/c_sex ='male') or (prefecture!=4 and  /data/person/c_sex ='female'))
and 
((prefecture!=2 and  /data/person/current_age &lt;12) or (prefecture&gt;0 and  /data/person/current_age &gt;=12))]">
          <value ref="name"/>
          <label ref="jr:itext(itextId)"/>
        </itemset>
      </select1>
      <input ref="/data/person/other_relationship">
        <label>Specify Relationship</label>
      </input>
      <select1 appearance="horizontal" ref="/data/person/client_category">
        <label>What is the client category?</label>
        <item>
          <label>National</label>
          <value>national</value>
        </item>
        <item>
          <label>Foreigner</label>
          <value>foreigner</value>
        </item>
        <item>
          <label>Refugee</label>
          <value>refugee</value>
        </item>
      </select1>
      <select1 ref="/data/person/nationality">
        <label>Nationality</label>
        <item>
          <label>Congolese</label>
          <value>congolese</value>
        </item>
        <item>
          <label>Rwandese</label>
          <value>rwandese</value>
        </item>
        <item>
          <label>South Sudanese</label>
          <value>s_sudanese</value>
        </item>
        <item>
          <label>Somali</label>
          <value>somali</value>
        </item>
        <item>
          <label>Burundian</label>
          <value>burundian</value>
        </item>
        <item>
          <label>Eritrean</label>
          <value>eritrean</value>
        </item>
        <item>
          <label>Ethiopian</label>
          <value>ethiopian</value>
        </item>
        <item>
          <label>Other</label>
          <value>other</value>
        </item>
      </select1>
      <input ref="/data/person/other_nationality">
        <label>Please specify nationality</label>
      </input>
      <select1 appearance="hidden" ref="/data/person/registered">
        <label>Is the client registered?</label>
        <item>
          <label>Yes</label>
          <value>yes</value>
        </item>
        <item>
          <label>No</label>
          <value>no</value>
        </item>
      </select1>
      <input appearance="hidden" ref="/data/person/individual_number">
        <label>Individual Number</label>
        <hint>Use mother's number if client is still an infant</hint>
      </input>
      <input ref="/data/person/group_number">
        <label>Group Number</label>
        <hint>Use mother's number if client is still an infant</hint>
      </input>
      <input ref="/data/person/nin">
        <label>NIN</label>
      </input>
      <select1 appearance="horizontal" ref="/data/person/p_has_disability">
        <label>Does the client have a disability?</label>
        <item>
          <label>Yes</label>
          <value>yes</value>
        </item>
        <item>
          <label>No</label>
          <value>no</value>
        </item>
      </select1>
      <select appearance="horizontal" ref="/data/person/disability">
        <label>Please select the disability category</label>
        <item>
          <label>Deafness (Difficulty in hearing even when using a hearing aid)</label>
          <value>deafness</value>
        </item>
        <item>
          <label>Lameness (Difficulty in walking or climbing steps)</label>
          <value>lameness</value>
        </item>
        <item>
          <label>Difficulty remembering (recalling events) or concentrating</label>
          <value>difficulty_remembering</value>
        </item>
        <item>
          <label>Blindness (Difficulty seeing even when wearing glasses or contact lenses)</label>
          <value>blindness</value>
        </item>
        <item>
          <label>Difficulty in Communicating (When Using his or her official language)</label>
          <value>difficulty_communicating</value>
        </item>
        <item>
          <label>Difficulty in self-care (Difficulty in washing all over or dressing)</label>
          <value>difficulty_selfcare</value>
        </item>
      </select>
      <select1 appearance="horizontal" ref="/data/person/child_in_school">
        <label>Is client in school?</label>
        <item>
          <label>Yes</label>
          <value>yes</value>
        </item>
        <item>
          <label>No</label>
          <value>no</value>
        </item>
      </select1>
      <select1 appearance="horizontal" ref="/data/person/p_test_for_hiv_last3months">
        <label>Has client tested for HIV in the last 3 months?</label>
        <item>
          <label>Yes</label>
          <value>yes</value>
        </item>
        <item>
          <label>No</label>
          <value>no</value>
        </item>
      </select1>
      <select1 ref="/data/person/p_hiv_test_result">
        <label><output value=" /data/person/hiv_test_result_label "/></label>
        <item>
          <label>Positive</label>
          <value>positive</value>
        </item>
        <item>
          <label>Negative</label>
          <value>negative</value>
        </item>
        <item>
          <label>Unknown</label>
          <value>unknown</value>
        </item>
      </select1>
      <select1 appearance="horizontal" ref="/data/person/p_on_art_treatment">
        <label>Is client on ART treatment?</label>
        <item>
          <label>Yes</label>
          <value>yes</value>
        </item>
        <item>
          <label>No</label>
          <value>no</value>
        </item>
      </select1>
      <input ref="/data/person/note_attend_art_clinic">
        <label>Encourage the client to attend ART clinic to begin treatment</label>
      </input>
      <select1 appearance="horizontal" ref="/data/person/taking_medication">
        <label>Is the client taking their medication as prescribed?</label>
        <item>
          <label>Yes</label>
          <value>yes</value>
        </item>
        <item>
          <label>No</label>
          <value>no</value>
        </item>
      </select1>
      <input ref="/data/person/note_explain_importance_taking_med">
        <label>Explain the importance of the client taking their medication as has been prescribed</label>
      </input>
      <input ref="/data/person/note_encourage_to_continue_taking_med">
        <label>Encourage the client to continue taking their medication as has been prescribed</label>
      </input>
      <input ref="/data/person/note_encourage_client">
        <label>Encourage the client to reduce their risk of getting HIV and retest after 3 months</label>
      </input>
      <input ref="/data/person/note_advise_client_check_status">
        <label>Advise client to check their status and start medication if positive or advise on prevention of HIV practices if negative</label>
      </input>
      <input ref="/data/person/note_advise_client_check_status2">
        <label>Encourage and refer the client to the health facility for HIV testing</label>
      </input>
      <select1 appearance="horizontal" ref="/data/person/p_has_tb">
        <label>Does the client have TB?</label>
        <item>
          <label>Yes</label>
          <value>yes</value>
        </item>
        <item>
          <label>No</label>
          <value>no</value>
        </item>
      </select1>
      <select1 appearance="horizontal" ref="/data/person/has_cough">
        <label>Have you had a cough for the last 2 weeks?</label>
        <item>
          <label>Yes</label>
          <value>yes</value>
        </item>
        <item>
          <label>No</label>
          <value>no</value>
        </item>
      </select1>
      <select1 appearance="horizontal" ref="/data/person/p_on_tb_treatment">
        <label>Is client on TB Treatment</label>
        <item>
          <label>Yes</label>
          <value>yes</value>
        </item>
        <item>
          <label>No</label>
          <value>no</value>
        </item>
      </select1>
      <input ref="/data/person/note_tb_referral">
        <label>Note: Refer client that is not on TB treatment to the health facility to seek help in order to reduce the risk of spreading TB on to other household members</label>
      </input>
      <select1 appearance="horizontal" ref="/data/person/received_hpv">
        <label>Has the client received HPV Vaccine?</label>
        <item>
          <label>Yes</label>
          <value>yes</value>
        </item>
        <item>
          <label>No</label>
          <value>no</value>
        </item>
      </select1>
      <select1 appearance="horizontal" ref="/data/person/received_tt_vaccine">
        <label>Has the client received TT vaccine?</label>
        <item>
          <label>Yes</label>
          <value>yes</value>
        </item>
        <item>
          <label>No</label>
          <value>no</value>
        </item>
      </select1>
      <select1 appearance="horizontal" ref="/data/person/has_hypertension">
        <label>Does the client have Hypertension?</label>
        <item>
          <label>Yes</label>
          <value>yes</value>
        </item>
        <item>
          <label>No</label>
          <value>no</value>
        </item>
        <item>
          <label>Unknown</label>
          <value>unknown</value>
        </item>
      </select1>
      <select1 appearance="horizontal" ref="/data/person/has_sickle_cell">
        <label>Does the client have Sickle Cell?</label>
        <item>
          <label>Yes</label>
          <value>yes</value>
        </item>
        <item>
          <label>No</label>
          <value>no</value>
        </item>
        <item>
          <label>Unknown</label>
          <value>unknown</value>
        </item>
      </select1>
      <select1 appearance="horizontal" ref="/data/person/has_diabetes">
        <label>Does the client have Diabetes?</label>
        <item>
          <label>Yes</label>
          <value>yes</value>
        </item>
        <item>
          <label>No</label>
          <value>no</value>
        </item>
        <item>
          <label>Unknown</label>
          <value>unknown</value>
        </item>
      </select1>
      <select1 appearance="horizontal" ref="/data/person/uses_tobacco">
        <label>Does client use Tobacco?</label>
        <item>
          <label>Yes</label>
          <value>yes</value>
        </item>
        <item>
          <label>No</label>
          <value>no</value>
        </item>
      </select1>
      <select1 appearance="horizontal" ref="/data/person/sleep_under_llin">
        <label>Is the client sleeping under an insecticide treated net (LLIN)?</label>
        <item>
          <label>Yes</label>
          <value>yes</value>
        </item>
        <item>
          <label>No</label>
          <value>no</value>
        </item>
      </select1>
      <select1 ref="/data/person/why_not_using_llin">
        <label>Why is the Client not sleeping under an insecticide treated net (LLIN)?</label>
        <item>
          <label>Don't have insecticide treated net (LLIN)</label>
          <value>has_no_llin</value>
        </item>
        <item>
          <label>There are not enough insecticide treated net (LLIN) for the number of people in household</label>
          <value>not_enough_llins</value>
        </item>
        <item>
          <label>Don't know how to hang/use insecticide treated net (LLIN)</label>
          <value>does_not_know_how_to_use</value>
        </item>
        <item>
          <label>Other (Please Specify)</label>
          <value>other</value>
        </item>
      </select1>
      <input ref="/data/person/why_not_using_llin_other">
        <label>Other (Please Specify)</label>
      </input>
      <input ref="/data/person/note_vht_assist_how_to_get_llin">
        <label>The VHT should assist or advice the client on how to get an insecticide treated net (LLIN)</label>
      </input>
      <input ref="/data/person/note_vht_demonstrate_on_llin_use">
        <label>The VHT should demonstrate how to hang an insecticide treated net (LLIN)</label>
      </input>
      <select1 appearance="horizontal" ref="/data/person/using_fp_method">
        <label>Is the client using any family planning method?</label>
        <item>
          <label>Yes</label>
          <value>yes</value>
        </item>
        <item>
          <label>No</label>
          <value>no</value>
        </item>
      </select1>
      <input ref="/data/person/note_fp_registration">
        <label>Please enroll them into care coordination using the Family Planning registration form</label>
      </input>
      <select1 ref="/data/person/menstrual_material">
        <label>What menstrual hygiene materials do you use?</label>
        <item>
          <label>Disposable sanitary pads</label>
          <value>disposable_pads</value>
        </item>
        <item>
          <label>Menstrual cups</label>
          <value>menstrual_cups</value>
        </item>
        <item>
          <label>Reusable sanitary pads</label>
          <value>reusable_pads</value>
        </item>
        <item>
          <label>Tampons</label>
          <value>tampons</value>
        </item>
        <item>
          <label>Cloths</label>
          <value>cloths</value>
        </item>
      </select1>
      <input ref="/data/person/mm_quantity">
        <label>How many <output value=" /data/person/menstrual_material_label "/> do you currently have?</label>
      </input>
      <select1 appearance="hidden" ref="/data/person/have_pads">
        <label>Do you have Disposable Sanitary pads?</label>
        <item>
          <label>Yes</label>
          <value>yes</value>
        </item>
        <item>
          <label>No</label>
          <value>no</value>
        </item>
      </select1>
